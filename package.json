{
  "name": "iso_8583",
  "version": "2.6.4",
  "description": "A javascript library for messaging in iso 8583 messaging format.",
  "main": "lib/8583.js",
  "scripts": {
    "pretest": "eslint --fix lib/*.js test/**",
    "docs": "jsdoc -c js_docs.conf.json .",
    "test": "nyc --reporter=html --reporter=lcov ava --verbose",
    "test.watch": " npx ava --verbose --watch"
  },
  "ava": {
    "files": [
      "test/*.js"
    ],
    "source": [
      "lib/**/*.{js}"
    ],
    "concurrency": 5,
    "failFast": true,
    "failWithoutAssertions": false,
    "powerAssert": false
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zemuldo/iso_8583.git"
  },
  "keywords": [
    "iso",
    "iso8583",
    "iso_8583",
    "card transaction",
    "8583"
  ],
  "author": "Danstan Otieno Onyango",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zemuldo/iso_8583/issues"
  },
  "homepage": "https://github.com/zemuldo/iso_8583/wiki",
  "dependencies": {
    "date-fns": "^2.0.0",
    "jxon": "^2.0.0-beta.5"
  },
  "devDependencies": {
    "ava": "^2.0.0",
    "eslint": "^5.0.0",
    "eslint-plugin-import": "^2.8.0",
    "gulp": "^4.0.2",
    "gulp-shell": "^0.7.1",
    "jsdoc": "^3.6.3",
    "nyc": "^14.0.0",
    "toast-jsdoc": "^1.0.2"
  }
}
